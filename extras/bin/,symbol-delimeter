#!/usr/bin/zsh 

path="/git/MyDotFiles/extras/bin/symbol-delimeter"

#only works in bash
IFS=';'
# syms=$(echo "➤ ➧ ➨ ➩ ➪ ➭ ➮ ➯ ➱ ➲ ➳ ➵ ➸ ➺ ➻ ➼ ➽ ➾ " | /usr/bin/tr ' ' "\n")
s="➤ ➧ ➨ ➩ ➪ ➭ ➮ ➯ ➱ ➲ ➳ ➵ ➸ ➺ ➻ ➼ ➽ ➾ 	→ ➔ ➙ ➛ ➜ ➝ ➞ ➟ ➠ ➡ ⇴ ⇰ ⇨ ⇥ ⇢ ⇝ ⇛ ⇒ ⇀ "
s+=" ⫐ ͻ ⁊ ⁍ ⯏ ⯍ ⁕ ⸭ ∷ ⸪ "
s+=" → ➔ ➙ ➛ ➜ ➝ ➞ ➟ ➠ ➡ ⇴ ⇰ ⇨ ⇥ ⇢ ⇝ ⇛ ⇒ ⇀ "
s+=" ↬ ↪ ↦ ↣ ↠ ↝ → ⁍ ⁃ › ‣ ‑ ‐ ͱ Ͱ ╼ ╺ ╶ ╌ ┈ ┉ ╍ ┅ ∙ "
s+="   ▶ ▷   ∙ ⸰ ∾ ⸛ ⸳ ⸴ ⹀ ⬥ ⬧ ⬩ ⬪ ⬝ "
s+=" ⬛     ⊢ ⊦ ⊧ ⊨ ⊩ ⊪ ⊫ ⊰ ⊳ ⊶ ⊷ ⊸ ⊐ ≻ ≫ ≎ ≖ ≡ ≣  ︰ ︓ ︙ ⌇ ︴ ⫴ ⫼  ⧘ ⧙ ⧚ ⧛ ⣤ ⠶ ⠛ ⣶ ⠿ ⣿  ⠘ ⠸  ⡇ "
s+=" ⩨ ⩩ ⬚ 〜 ∼ ∽ ≈ ≋   ∿  ⯓  ⯡	 ⯖ "
s+="      		        	           		         "
s+="    "
s+="┃ │ ▕ ┋ ┆ ⸽ ⸾ ┊  ┇ ╏ ╎  ⁞ ⁝ "
s+="⧽ ❭ ❯ ❱ ⫸  》 〉 ≻  ≫  "
s+="	 ╱ ⧸ ∕    ⃫ ⁄ ⸙ ⸗ "
s+=" ─ ━ 	╌ ┄ ┈ ╍ ┉ ┅    ⑉ ⚊  ⚋  "
s+=" ‐ ‑ – ‒ ─ ━ — ―  ╴ ╶ ╸ ╺ ╼ "
s+="  ■	 ◼	 ◾  □ ◻ ◽  ⃞	    ⃱   ⃢ ☐     ⃝	 ◯	 ○	 ◌	 ●	 ∙ ♠ ♣  ♥ ♦  "
s+=" □ ■	 ▢  ▣  ◻ ◼ ◽ ◾ ◿  ◺  ◸  ◹ ◢	 ◣  ◤ ◥  ◖ ◗    "

# syms=(${s// / })
syms=($(echo $s| /usr/bin/tr " " ";"))
/usr/bin/clear
w=231
g=247
bg=239

print -P "%F{$w}"
for ch in $syms
do
   rep="%F{$g}%B%K{$bg}$ch %b%F{$w}%K{$bg}"
   print -P "%K{096}   $ch %K{$bg}%F{096} %F{$w}  ${path//\// $rep } %k%F{$bg}%f\n"
done
